<h1>Listing cashrecords</h1>

<table>
  <tr>
  </tr>

<% for cashrecord in @cashrecords %>
  <tr>
    <td><%= link_to 'Show', cashrecord %></td>
    <td><%= link_to 'Edit', edit_cashrecord_path(cashrecord) %></td>
    <td><%= link_to 'Destroy', cashrecord, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New cashrecord', new_cashrecord_path %>
